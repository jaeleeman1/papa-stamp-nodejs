  <script src='https://maps.googleapis.com/maps/api/js'></script>
</head>

<body>
<div class="wrapper">
  <div class="top-status-bar">

    <div class="top-icon">
      <img class="stamp_icon" src="/images/papastamp_icon.png"></img>
    </div>
    <div class="top-title">파파스탬프</div>
    <div class="top-description">온라인 스탬프<br>
      관리 시스템</div>
  </div>
  <div class="content">
    <div class="phone" ng-app="delivcard">
      <div class="phone__screen">
        <div class="phone__scroll-cont">
          <div class="phone__content" ng-controller="DelivCtrl">

            <div class="all">
              <div class="cards">
                <div class="card shadow-2">
                  <div class="container">
                    <div class="card">
                      <figure class="front">
                        <img class="baseimg" src="http://www.jboeijenga.nl/img/back.jpg" alt="back">
                        <div class="img-repeat">
                          <div style="border: 1px solid gold; float: left; width: 20%; margin-bottom:5px;">
                            <img class="stamp-img" src="/images/stamp.png"  style="display:block;"></img>
                          </div>
                          <div style="border: 1px solid red; float: left; width: 20%; margin-bottom:5px;">
                            <img class="stamp-img" src="/images/stamp.png"  style="display:block;"></img>
                          </div>
                          <div style="border: 1px solid blue; float: left; width: 20%; margin-bottom:5px;">
                            <img class="stamp-img" src="/images/stamp.png"  style="display:block;"></img>
                          </div>
                          <div style="border: 1px solid blue; float: left; width: 20%; margin-bottom:5px;">
                            <img class="stamp-img" src="/images/stamp.png"  style="display:block;"></img>
                          </div>
                          <div style="border: 1px solid blue; float: left; width: 20%; margin-bottom:5px;">
                            <img class="stamp-img" src="/images/stamp.png"  style="display:block;"></img>
                          </div>
                        </div>
                        <div class="img-repeat">
                          <div style="border: 1px solid gold; float: left; width: 20%;">
                            <img class="stamp-img" src="/images/stamp.png"  style="display:block;"></img>
                          </div>
                          <div style="border: 1px solid red; float: left; width: 20%;">
                            <img class="stamp-img" src="/images/stamp.png"  style="display:block;"></img>
                          </div>
                          <div style="border: 1px solid blue; float: left; width: 20%;">
                            <img class="stamp-img" src="/images/stamp.png"  style="display:block;"></img>
                          </div>
                          <div style="border: 1px solid blue; float: left; width: 20%;">
                            <img class="stamp-img" src="/images/stamp.png"  style="display:block;"></img>
                          </div>
                          <div style="border: 1px solid blue; float: left; width: 20%;">
                            <img class="stamp-img" src="/images/stamp.png"  style="display:block;"></img>
                          </div>
                        </div>
                      </figure>

                      <figure class="back">
                        <img class="baseimg" src="http://www.jboeijenga.nl/img/front.jpg" alt="front">
                        <div class="caption">
                          <h2>john <span>doe</span></h2>
                          <p>web developer</p>
                          <a href="#">View more</a>
                        </div>
                      </figure>
                    </div>
                  </div>
                </div>

                <div class="card shadow-2">
                  <div class="container">
                    <div class="card">
                      <figure class="front">
                        <img class="baseimg" src="http://www.jboeijenga.nl/img/back.jpg" alt="back">
                        <div class="img-repeat">
                          <div style="border: 1px solid gold; float: left; width: 20%; margin-bottom:5px;">
                            <img class="stamp-img" src="/images/stamp.png"  style="display:block;"></img>
                          </div>
                          <div style="border: 1px solid red; float: left; width: 20%; margin-bottom:5px;">
                            <img class="stamp-img" src="/images/stamp.png"  style="display:block;"></img>
