<script type="text/javascript">
    // somebody should clean up this mess
    var setCardTransform = function setCardTransform(cardIndex, angle){
        var card = $('.card-list .card').eq(cardIndex).css('transform', 'rotate3d(1, 0, 0, ' + angle + 'deg)');
        $('.card-list .card').not(card).css('transform', 'none');
    };

    $(function () {
        var $window = $(window);
        var windowHeight = $window.height();

        $window.resize(function (e) {
            windowHeight = $window.height();
        });
        $window.resize(function (e) {
            var cardPercent = (window.scrollY % (windowHeight / 2) / windowHeight * 2);
            var cardCount = Math.floor(window.scrollY / (windowHeight / 2));
            setCardTransform(cardCount, cardPercent * 50);
        });
    });
</script>